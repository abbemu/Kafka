{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KafkaSettings": {
    "BootstrapServers": "localhost:9092",
    "Topic": "orders",
    "GroupId": "order-consumers",
    "MessageTimeoutMs": 5000,
    "AutoOffsetReset": "Earliest",
    "EnableAutoCommit": true,
    "MaxRetries": 3
  },
  
  "AllowedHosts": "*"
}
